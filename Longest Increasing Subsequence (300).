
#O (n^2- dp) +  0(n) space
class Solution:
    def lengthOfLIS(self, nums: List[int]) -> int:
        n=len(nums)
        if n<2:
            return n
        l=list()
        for i in range (n):
            l.append(1)
        for i in range (n):
            for j in range (i):
                if(nums[i]>nums[j] and 1+l[j]>l[i]):
                    l[i]=1+l[j]
        return max(l)
       
        
